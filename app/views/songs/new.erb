  <h1>Make a new Song</h1>

  <form action="/songs" method="post">
    Song Name: <input type="text", name="song[name]"><br>
    Song Artist: <input type="text", name="song[artist][name]"><br>

    Song Genre:<br>
    <% @genres.each do |genre| %>
    <%=genre.name%> <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>"><br>
    <% end %>
    <input type="submit">
  </form>
